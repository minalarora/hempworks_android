<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_navigation"
    app:startDestination="@id/homeFragment">


    <fragment
        android:id="@+id/homeFragment"
        android:name="com.hemp.works.dashboard.home.ui.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/homeFragment"/>
        <action
            android:id="@+id/action_homeFragment_to_searchFragment"
            app:destination="@id/searchFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/homeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_productListFragment"
            app:destination="@id/productListFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/homeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_homeFragment_to_prescriptionFragment"
            app:destination="@id/prescriptionFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />

        <action
            android:id="@+id/action_homeFragment_to_dosageCalculatorFragment"
            app:destination="@id/dosageCalculatorFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_homeFragment_to_TNLFragment"
            app:destination="@id/TNLFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_homeFragment_to_cartFragment"
            app:destination="@id/cartFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />

        <action
            android:id="@+id/action_homeFragment_to_orderFragment"
            app:destination="@id/orderFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_homeFragment_to_supportFragment"
            app:destination="@id/supportFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_homeFragment_to_notificationFragment"
            app:destination="@id/notificationFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_homeFragment_to_paymentFragment"
            app:destination="@id/paymentFragment"
            app:popUpTo="@id/homeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />

    </fragment>
    <fragment
        android:id="@+id/productFragment"
        android:name="com.hemp.works.dashboard.product.ui.ProductFragment"
        android:label="ProductFragment" >
        <argument
            android:name="id"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_productFragment_to_productImageFragment"
            app:destination="@id/productImageFragment"
            app:enterAnim="@anim/fade_in"
            app:popUpTo="@id/productFragment"
            app:popExitAnim="@anim/fade_out" />

        <action
            android:id="@+id/action_productFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/fade_in"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_productFragment_to_cartFragment"
            app:destination="@id/cartFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.hemp.works.dashboard.search.ui.SearchFragment"
        android:label="SearchFragment" >
        <action
            android:id="@+id/action_searchFragment_to_productListFragment"
            app:destination="@id/productListFragment"
            app:popUpTo="@id/searchFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/productListFragment"
        android:name="com.hemp.works.dashboard.home.ui.ProductListFragment"
        android:label="ProductListFragment" >
        <argument
            android:name="product"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="category"
            app:argType="string"
            app:nullable="true" />

        <action
            android:id="@+id/action_productListFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popUpTo="@id/homeFragment"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/productImageFragment"
        android:name="com.hemp.works.dashboard.product.ui.ProductImageFragment"
        android:label="ProductImageFragment" >

        <argument
            android:name="url"
            app:argType="string"
            app:nullable="true" />

    </fragment>
    <dialog
        android:id="@+id/infoBottomSheetFragment"
        android:name="com.hemp.works.dashboard.info.InfoBottomSheetFragment"
        android:label="InfoBottomSheetFragment" >
        <argument
            android:name="text"
            app:argType="string"
            app:nullable="false" />
    </dialog>
    <action
        android:id="@+id/action_global_infoBottomSheetFragment"
        app:destination="@id/infoBottomSheetFragment" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.hemp.works.dashboard.profile.ui.ProfileFragment"
        android:label="ProfileFragment" />
    <fragment
        android:id="@+id/prescriptionFragment"
        android:name="com.hemp.works.dashboard.prescription.ui.PrescriptionFragment"
        android:label="PrescriptionFragment" >
        <action
            android:id="@+id/action_prescriptionFragment_to_uploadPrescriptionFragment"
            app:destination="@id/uploadPrescriptionFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/uploadPrescriptionFragment"
        android:name="com.hemp.works.dashboard.prescription.ui.UploadPrescriptionFragment"
        android:label="UploadPrescriptionFragment" />
    <fragment
        android:id="@+id/dosageCalculatorFragment"
        android:name="com.hemp.works.dashboard.calculator.ui.DosageCalculatorFragment"
        android:label="DosageCalculatorFragment" />
    <fragment
        android:id="@+id/infoDialogFragment"
        android:name="com.hemp.works.dashboard.info.InfoDialogFragment"
        android:label="InfoDialogFragment" >
        <argument
            android:name="text"
            app:argType="string"
            app:nullable="false" />
    </fragment>
    <action
        android:id="@+id/action_global_infoDialogFragment"
        app:destination="@id/infoDialogFragment" />
    <fragment
        android:id="@+id/TNLFragment"
        android:name="com.hemp.works.dashboard.tnl.ui.TNLFragment"
        android:label="fragment_tnl"
        tools:layout="@layout/fragment_tnl" >
        <argument
            android:name="type"
            app:argType="string"
            app:nullable="false" />
    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.hemp.works.dashboard.cart.ui.CartFragment"
        android:label="CartFragment" >
        <action
            android:id="@+id/action_cartFragment_to_couponFragment"
            app:destination="@id/couponFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_right"
            app:popExitAnim="@anim/slide_out_right" />

        <action
            android:id="@+id/action_cartFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_right"
            app:popExitAnim="@anim/slide_out_right" />

        <action
            android:id="@+id/action_cartFragment_to_addressFragment"
            app:destination="@id/addressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/couponFragment"
        android:name="com.hemp.works.dashboard.cart.ui.CouponFragment"
        android:label="CouponFragment" />
    <fragment
        android:id="@+id/addressFragment"
        android:name="com.hemp.works.dashboard.address.ui.AddressFragment"
        android:label="AddressFragment"
        tools:layout="@layout/fragment_address" >
        <action
            android:id="@+id/action_addressFragment_to_createAddressFragment"
            app:destination="@id/createAddressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_addressFragment_to_paymentFragment"
            app:destination="@id/paymentFragment"
            app:popUpTo="@id/homeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/createAddressFragment"
        android:name="com.hemp.works.dashboard.address.ui.CreateAddressFragment"
        android:label="CreateAddressFragment" >
        <argument
            android:name="id"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/paymentFragment"
        android:name="com.hemp.works.dashboard.payment.ui.PaymentFragment"
        android:label="PaymentFragment"
        tools:layout="@layout/fragment_payment" >
        <argument
            android:name="payment"
            app:argType="com.hemp.works.dashboard.model.RequestPayment"
            app:nullable="false"/>
        <action
            android:id="@+id/action_paymentFragment_to_supportFragment"
            app:destination="@id/supportFragment"
            app:popUpTo="@id/homeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/orderFragment"
        android:name="com.hemp.works.dashboard.order.ui.OrderFragment"
        android:label="OrderFragment">

        <action
            android:id="@+id/action_orderFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/supportFragment"
        android:name="com.hemp.works.dashboard.support.ui.SupportFragment"
        android:label="SupportFragment" />
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.hemp.works.dashboard.notification.ui.NotificationFragment"
        android:label="NotificationFragment" />

</navigation>
<!--https://developer.android.com/guide/navigation/navigation-pass-data#supported_argument_types-->